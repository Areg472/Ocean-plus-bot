{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python@3.1.0",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "JEYY_API": "@JEYY_API", 
    "GEMINI_API_KEY": "@GEMINI_API_KEY",
    "TOKEN": "@TOKEN"
  },
  "functions": {
    "api/": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}